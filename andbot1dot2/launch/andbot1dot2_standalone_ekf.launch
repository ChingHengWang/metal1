<?xml version="1.0"?>
<launch>
 <!-- change this next line to include the launch file for your robot's hardware drivers: -->

	<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
	    <param name="port" value="/dev/ttyACM0"/>
	    <param name="baud" value="57600"/></node>

  <!-- urdf xml robot description loaded on the Parameter Server-->
	<param name="robot_description" command="$(find xacro)/xacro '$(find andbot1dot2)/modeldescription/urdf/angelbot.xacro'" />
	
	<!-- Robot parameters -->
	<param name="wheelSeparation" value="0.41367"></param>
	<param name="wheelRadius" value="0.085"></param>
	<param name="rate" value="10"></param>  
	
	<!-- Starting Joint state publisher node which will publish the joint values -->
	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
	<!-- Starting robot state publisher -->
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher">
		<param name="publish_frequency" type="double" value="50.0"></param></node>
		
	<!-- Starting twist to motor and diff_tf nodes -->
	<node pkg="andbot1dot2" type="mybot_base_controller_v1_andbot1dot2" name="mybot_base_controller_v1_andbot1dot2" output="screen"/>
 	<node pkg="andbot1dot2" type="mybot_odom_ekf_andbot1dot2" name="mybot_odom_ekf_andbot1dot2" output="screen"/>
 	 
 	 <!-- Starting robot_pose_ekf nodes -->
	<include file="$(find andbot1dot2)/launch/robot_pose_ekf.launch" />

</launch>