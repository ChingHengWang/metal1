<?xml version="1.0"?>


<launch>
	<!-- run serial node for base mega -->	
	<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    		<param name="port" value="/dev/ttyACM0"/>
    		<param name="baud" value="1000000"/>
  	</node>

	<include file="$(find andbot)/launch/arm.launch" />


</launch>
